<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.1.dtd" >
<suite name="flight-reservation">
    <test name = "passengers-count-1">
        <parameter name = "noOfPassengers" value="1"></parameter>
        <parameter name = "expectedPrice" value="$584 USD"></parameter>
        <classes>
            <class name = "org.example.automation_testdemo.tests.flightreservation.FlightReservationTest"></class>
        </classes>
    </test>
    <test name = "passengers-count-2">
        <parameter name = "noOfPassengers" value="2"></parameter>
        <parameter name = "expectedPrice" value="$1169 USD"></parameter>
        <classes>
            <class name = "org.example.automation_testdemo.tests.flightreservation.FlightReservationTest"></class>
        </classes>
    </test>
</suite>